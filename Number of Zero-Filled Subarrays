class Solution:
    def zeroFilledSubarray(self, nums: List[int]) -> int:
        total = 0
        count = 0
        
        for num in nums:
            if num == 0:
                count += 1
                total += count  # each new zero extends all previous zero-subarrays
            else:
                count = 0  # reset when non-zero found
                
        return total
