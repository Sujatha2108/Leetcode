class Solution:
    def isOneBitCharacter(self, bits: List[int]) -> bool:
        i = 0
        n = len(bits)
        while i < n - 1:  # we stop before the last bit
            if bits[i] == 1:
                i += 2  # skip this 2-bit character
            else:
                i += 1  # skip this 1-bit character
        return i == n - 1  # if i ends on the last bit, it's a one-bit character
